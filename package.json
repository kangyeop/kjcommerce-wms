{
  "name": "kjcommerce-wms",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter @kjcommerce-wms/web dev",
    "dev:server": "pnpm --filter @kjcommerce-wms/server dev",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm dev:server\"",
    "build": "pnpm --filter @kjcommerce-wms/web build",
    "build:server": "pnpm --filter @kjcommerce-wms/server build",
    "build:all": "pnpm build && pnpm build:server",
    "preview": "pnpm --filter @kjcommerce-wms/web preview",
    "start": "pnpm --filter @kjcommerce-wms/server start"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
